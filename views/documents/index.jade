h1 Your Documents

p
  a(class='button', href='/documents/new') + New Document

ul
  - for (var d in applications)
    li
      a(class='button', href='/documents/' + applications[d].id) Edit &nbsp;
      a(class='button destroy', href='/documents/' + applications[d].id) Delete &nbsp;
      a(href='/documents/' + applications[d].id)= applications[d].title


